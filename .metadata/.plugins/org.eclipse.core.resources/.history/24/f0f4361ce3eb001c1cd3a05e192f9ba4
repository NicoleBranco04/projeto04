package aplicacao;

import classes.Cliente;

public class appCliente {

	public static void main(String[] args) {
	
		Cliente cli = new Cliente();
		cli.nome = "Pedro";
		cli.email = "pedro@terra.com.br";
		cli.cpf = "9943215521";		
        cli.cadastrar(cli);
        System.out.println("-----------Cliente cadastrados------------");
        cli.listar();
	}

}
